  <li class="<%= gray?(dt) %>" >
    <div>
      <a href="<%= url %>"><%= name %></a>
    </div>
    <div> тнР <%= stars %></div>
    <div> ЁЯУЕ <%= days(dt) %></div>
  </li>
<% end %>
</ul>
